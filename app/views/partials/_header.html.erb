<header class="header">

  <div class="bg-image"></div>

  <div class="row topbar">

    <div class="col-lg-6">
      <div class="logo-container">
        <%= link_to root_path do %>
          <%= image_tag "logo.png", class: 'logo' %>
        <% end %>
      </div>
    </div>
    
    <div class="col-lg-6">
      
      <div class="login" align="right">
        <nav class="login-menu">
          <ul>
            <li>
              <a class="aregistros" style="color:white">
                <img  src="<%= image_url("11.png")%>" alt="">
                <%= link_to 'INICIAR', new_user_session_path, class: 'link' %>
              </a>
            </li>

            <li>
              <a class="aregistros" style="color:white" >
                <img  src="<%= image_url("12.png")%>" alt="">
                <%= link_to 'REGISTRARSE', new_user_registration_path, class: 'link' %>
              </a>
            </li>
          </ul>
        </nav>

      </div>
    </div>
  </div>
</header>